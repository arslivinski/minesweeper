{
  "presets": [
    ["@babel/preset-react", { "runtime": "automatic" }],
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "bugfixes": true,
        "corejs": 3
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-react", { "development": true, "runtime": "automatic" }],
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "entry",
            "bugfixes": true,
            "corejs": 3
          }
        ]
      ],
      "plugins": ["@babel/plugin-proposal-class-properties"]
    }
  }
}
